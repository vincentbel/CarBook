        </activity>

        <activity
                android:name="com.Doric.CarBook.car.CarShow"
                android:label="carShow"
                android:screenOrientation="portrait">
        </activity>
        <!--<activity android:name="com.Doric.CarBook.member.UserCollection"
                  android:label="userCollection"
                  android:theme="@style/Theme.CarShow"
                  android:screenOrientation="portrait">
        </activity>-->

        <activity
                android:name="com.Doric.CarBook.search.SearchMain"
                android:screenOrientation="portrait">
        </activity>

